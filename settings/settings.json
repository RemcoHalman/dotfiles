{
  "workbench.iconTheme": "material-icon-theme",
  "material-icon-theme.folders.associations": {
    "ui": "layout",
    "global_state": "global"
  },
  "workbench.colorCustomizations": {
    "editor.selectionBackground": "#135564",
    "editor.selectionHighlightBackground": "#135564"
  },
  "editor.fontSize": 16,
  "editor.fontWeight": "500",
  "editor.fontFamily": "Source Code Pro",
  "debug.console.fontFamily": "Source Code Pro",
  "debug.console.fontSize": 15,
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontWeight": "600",
  "editor.formatOnSave": true,
  "editor.tokenColorCustomizations": {
    "comments": "#60ff7d"
  },
  "window.zoomLevel": 0,
  "python.pythonPath": "/usr/local/bin/python3",
  "code-runner.executorMap": {
    "python": "$pythonPath -u $fullFileName",
    "html": "open -b com.google.Chrome"
  },
  "code-runner.clearPreviousOutput": true,
  "code-runner.showExecutionMessage": false,
  "[plaintext]": {
    "editor.quickSuggestions": false
  },
  "editor.quickSuggestionsDelay": 100,
  "zenMode.centerLayout": false,
  "zenMode.fullScreen": true,
  "zenMode.hideLineNumbers": false,
  "zenMode.hideTabs": false,
  "zenMode.hideActivityBar": true,
  "editor.minimap.enabled": false,
  "workbench.settings.openDefaultSettings": true,
  "workbench.settings.useSplitJSON": true,
  "workbench.settings.editor": "json",
  "files.autoSave": "afterDelay",
  "files.exclude": {
    "**/.DS_Store": false,
    "**/.git": false
  },
  "files.autoSaveDelay": 4000,
  "pandoc.docxOptString": "",
  "pandoc.htmlOptString": "-s -t html5",
  "pandoc.pdfOptString": "",
  "terminal.integrated.rendererType": "dom",
  "python.jediEnabled": true,
  "[markdown]": {
    "editor.defaultFormatter": "darkriszty.markdown-table-prettify"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDragAndDrop": false,
  "workbench.activityBar.visible": true,
  "workbench.statusBar.visible": true,
  "C_Cpp.updateChannel": "Insiders",
  "dart.previewFlutterUiGuides": true,
  "dart.previewFlutterUiGuidesCustomTracking": true,
  "todo-tree.general.tags": ["TODO", "FIXME", "FUTURE"],
  "beautify.language": {
    "html": ["htm", "html", "django-html"]
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "liveServer.settings.port": 6750,
  "[json]": {
    "editor.defaultFormatter": "HookyQR.beautify"
  },
  "bracketPairColorizer.consecutivePairColors": [
    "<>",
    "()",
    "[]",
    "{}",
    ["Green", "Orchid", "Yellow"],
    "Red"
  ],
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "workbench.colorTheme": "Monokai",
  "cSpell.language": "en,nl",
  "cSpell.userWords": ["CRGB", "MQTT", "nbsp", "println"],
  "[scss]": {
    "editor.defaultFormatter": "HookyQR.beautify"
  },
  "editor.renderWhitespace": "all",
  "todo-tree.tree.showScanModeButton": false
}
